{
	"name": "df_derived_date_parts",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "ls_for_ADLS_for_input",
						"type": "LinkedServiceReference"
					},
					"name": "srcemployee"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "ls_for_ADLS_for_input",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmployeeNo as short,",
				"          Ename as string,",
				"          Job as string,",
				"          Mgr as string,",
				"          Hiredate as date,",
				"          Salary as double,",
				"          Commission as double,",
				"          DepartmentNo as short",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'datafile',",
				"     folderPath: 'input',",
				"     fileName: 'employee.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> srcemployee",
				"srcemployee derive(year = year(Hiredate),",
				"          semister = iif(month(Hiredate)<=6,\"H1\",\"H2\"),",
				"          quarter = case(\r",
				"    month(Hiredate)<=3,\"Q1\",\r",
				"    month(Hiredate)>3 && month(Hiredate)<=6,\"Q2\",\r",
				"    month(Hiredate)>36 && month(Hiredate)<=9,\"Q3\",\r",
				"    \"Q4\"),",
				"          month = case(\r",
				"    month(Hiredate)==1,\"Jan\",\r",
				"     month(Hiredate)==2,\"Feb\",\r",
				"      month(Hiredate)==3,\"March\",\r",
				"       month(Hiredate)==4,\"April\",\r",
				"        month(Hiredate)==5,\"May\",\r",
				"         month(Hiredate)==6,\"June\",\r",
				"          month(Hiredate)==7,\"July\",\r",
				"           month(Hiredate)==8,\"Aug\",\r",
				"            month(Hiredate)==9,\"Sep\",\r",
				"             month(Hiredate)==10,\"Oct\",\r",
				"             month(Hiredate)==11,\"Nov\",\r",
				"              month(Hiredate)==12,\"Dec\"\r",
				"              )) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delimited',",
				"     fileSystem: 'datafile',",
				"     folderPath: 'output/employee_dateparts',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}